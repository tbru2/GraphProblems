class KnightsTour{
    private int [][[] board;
    private int size;
    private Stack<Integer> stack;
                   
        public KnightsTour(int n){
            int count = 0;
            size = n;
            board = new int[size][size];
            for(int i = 0;i< size;i++){
                for(int j = 0; j< size;j++){
                    board[i][j] = count++;
                }
            }
       }
}